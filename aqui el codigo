diccionario_de_cosas = {
    "cringe": "vergonzoso o que genera incomodidad",
    "LOL":"gracioso o chistoso",
    "hate":"odio o absoluto desagrado",
    "funa":"denuncia publica contra una persona individual o juridica",
    "trolear":"hacer una broma o burlarse de alguien a alguien",
    "WTF":"expresión de confusión",
    "streamer":"perona que hace directos"
}
word = input("Escribe una palabra que no entiendas: ")
if word in diccionario_de_cosas.keys():
    print(diccionario_de_cosas[word])
else:
    print("la palabra no está en este diccionario")
